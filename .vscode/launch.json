  "version": "0.2.0",
  "configurations": [
    {
      "name": "App Dev (Android)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": [
        "--flavor",
        "dev",
        "--dart-define=APP_MODE=dev"
      ]
    },
    {
      "name": "App Prod (Android)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": [
        "--flavor",
        "production",
        "--dart-define=APP_MODE=production",
        "--dart-define=GATEWAY_BOOTNODES_URL=https://bootnodes.vocdoni.net/gateways.json",
        "--dart-define=NETWORK_ID=xdai",
        "--dart-define=LINKING_DOMAIN=vocdoni.link"
      ]
    },
    {
      "name": "App Dev (iOS)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": [
        "--dart-define=APP_MODE=dev",
      ]
    },
    {
      "name": "App Beta (iOS)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": [
        "--dart-define=APP_MODE=beta",
        "--dart-define=GATEWAY_BOOTNODES_URL=https://bootnodes.vocdoni.net/gateways.stg.json",
        "--dart-define=NETWORK_ID=xdai",
        "--dart-define=LINKING_DOMAIN=vocdonistg.link"
      ]
    },
    {
      "name": "App Prod (iOS)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": [
        "--dart-define=APP_MODE=production",
        "--dart-define=GATEWAY_BOOTNODES_URL=https://bootnodes.vocdoni.net/gateways.json",
        "--dart-define=NETWORK_ID=xdai",
        "--dart-define=LINKING_DOMAIN=vocdoni.link"
      ]
    },
    {
      "name": "App Profile (Android)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": [
        "--flavor",
        "production",
        "--dart-define=APP_MODE=dev",
      ]
    }
  ]
}